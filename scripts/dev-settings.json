{
  "target": "pub",
  "defaultVersion": "1.0.0",
  "copyFolders": ["assets", "images", "workers"],
  "entryPoints": ["index.js", "index.ts", "index.jsx", "index.tsx"],
  "excludeEntryFolders": ["node_modules", "dist", "pub"],
  "externals": [
    "*.png",
    "*.jpg",
    "*.gif",
    "*.bmp",
    "*.webp",
    "*.svg",
    "*.ttf",
    "*.woff",
    "*.woff2",
    "*.eot",
    "*.otf"
  ],
  "externalRules": {
    "^wpa$": {
      "prod": "/libs/wpa/latest/wpa.js",
      "dev": "/node_modules/wpa/dist/wpa.js"
    },
    "^wpa-ui$": "/node_modules/wpa-ui/dist/wpa-ui.js",
    "^vue@2.7.16": "/libs/vue/2.7.16/esm/vue.min.js",
    "^/(.+)": {
      "buildIn": false,
      "dev": "/$1/index.js",
      "prod": "/$1/"
    }
  },
  "pub": {
    "server": {
      "port": 7903
    }
  },
  "dev": {
    "server": {
      "port": 7902
    },
    "remotes": [
      {
        "host": "localhost",
        "port": 7903,
        "from": "/",
        "to": "/"
      }
    ],
    "apis": [
      {
        "host": "localhost",
        "port": 8080,
        "from": "/api/",
        "to": "/"
      }
    ]
  }
}
