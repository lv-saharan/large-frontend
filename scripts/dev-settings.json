{
  "externals": [
    "*.png",
    "*.jpg",
    "*.gif",
    "*.bmp",
    "*.webp",
    "*.svg",
    "*.ttf",
    "*.woff",
    "*.woff2",
    "*.eot",
    "*.otf"
  ],
  "externalRules": {
    "^wpa$": "/libs/wpa/1.0.13/wpa.js",
    "^wpa-ui$": "/libs/wpa-ui/1.0.10/wpa-ui.js",
    "^vue@2.7.16": "/libs/vue/2.7.16/esm/vue.min.js",
