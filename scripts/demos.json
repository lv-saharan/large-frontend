{
  "externals": {
    "^wpa$": {
      "dev": "/node_modules/wpa/dist/wpa.js",
      "prod": "/node_modules/wpa/dist/wpa.js"
    },
    "^wpa-ui$": {
      "dev": "/node_modules/wpa-ui/dist/wpa-ui.js",
      "prod": "/node_modules/wpa-ui/dist/wpa-ui.js"
    },
    "^~/": {
      "dev": "/",
      "prod": "/"
    }
  },
  "localDev": {
    "server": {
      "port": 7902
    },
    "apis": [
      {
        "host": "localhost",
        "port": 9192,
        "from": "/api/",
        "to": "/"
      }
    ]
  }
}